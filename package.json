{
  "name": "sequelize-abs-migrate",
  "version": "1.0.0",
  "publishConfig": {
    "tag": "latest"
  },
  "description": "Automatically generate bare sequelize models & Migrations from your database.",
  "main": "index.js",
  "types": "types",
  "keywords": [
    "mysql",
    "mssql",
    "sqlite",
    "postgres",
    "sequelize",
    "sequelizejs",
    "generator",
    "mapper",
    "typescript"
  ],
  "files": [
    "*.js",
    "lib/**/*.js",
    "types/**/*.ts",
    "*.md"
  ],
  "bin": {
    "sequelize-abs-migrate": "bin/sequelize-abs-migrate"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/muthu322/sequelize-abs-migrate.git"
  },
  "bugs": {
    "url": "https://github.com/muthu322/sequelize-abs-migrate/issues"
  },
  "nyc": {
    "exclude": [
      "**/test/*"
    ]
  },
  "scripts": {
    "tsc": "tsc",
    "clean": "rimraf lib && rimraf types && rimraf **/models && rimraf *.tgz",
    "build": "npm run clean && tsc",
    "prepack": "crlf --set=LF bin/sequelize-abs-migrate",
    "test": "npm run test-mssql && npm run test-mysql && npm run test-postgres && npm run test-sqlite",
    "test-postgres": "cross-env DEBUG='sequelize-abs-migrate:*' DIALECT=postgres nyc mocha",
    "test-postgres-native": "cross-env DEBUG='sequelize-abs-migrate:*' DIALECT=postgres-native nyc mocha",
    "test-mysql": "cross-env DEBUG='sequelize-abs-migrate:*' DIALECT=mysql nyc mocha",
    "test-sqlite": "cross-env DEBUG='sequelize-abs-migrate:*' DIALECT=sqlite nyc mocha",
    "test-mssql": "cross-env DEBUG='sequelize-abs-migrate:*' DIALECT=mssql nyc mocha",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "engines": {
    "node": ">= 10"
  },
  "author": "Steve Schmitt (https://github.com/steveschmitt)",
  "contributors": [
    "Daniel Durante <me@danieldurante.com>",
    "Yuping Zuo (https://github.com/zypA13510)"
  ],
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "yargs": "^16.2.0"
  },
  "peerDependencies": {
    "sequelize": ">3.30.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.17",
    "@types/lodash": "^4.14.168",
    "@types/mocha": "^8.2.2",
    "@types/reserved-words": "0.1.0",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^3.10.1",
    "chai": "^4.3.4",
    "coveralls": "^3.1.0",
    "crlf": "^1.1.1",
    "cross-env": "^7.0.3",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "lcov-result-merger": "^3.1.0",
    "mocha": "^8.3.2",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "nyc": "^15.1.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "reserved-words": "^0.1.2",
    "rimraf": "^3.0.2",
    "sequelize": "^6.11",
    "sqlite3": "5.0.2",
    "tedious": "^11.0.6",
    "typescript": "^4.2.3"
  }
}
